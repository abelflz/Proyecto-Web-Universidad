@model IEnumerable<Universidad.ServiceReference1.ArrayOfString>
@{
    ViewBag.Title = "VerCurso";
}
<table class="table-bordered table-striped">
    <tr>
        <th>
            @Html.Label("Nombre Materia")
        </th>
        <th>
            @Html.Label("Créditos")
        </th>
        <th>
            @Html.Label("Código")
        </th>
    </tr>
    @foreach(var i in Model)
    {
        <tr>
            <td>
                @i.
            </td>
        </tr>
    }
</table>

